{
  "name": "@wcp/wario-ux-shared",
  "version": "4.0.1",
  "description": "Shared UX elements across WARIO",
  "repository": {
    "type": "git",
    "url": "https://github.com/laviddichterman/wario-monorepo.git",
    "directory": "packages/wario-ux-shared"
  },
  "license": "GPL-3.0-or-later",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./phone-metadata.custom.json": {
      "default": "./dist/phone-metadata.custom.json"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "pnpm run generate:metadata",
    "build": "vite build",
    "generate:metadata": "libphonenumber-metadata-generator src/generated/phone-metadata.custom.json --countries US,CA --extended --types mobile,fixed_line,voip",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/geojson": "^7946.0.16",
    "@types/mailcheck": "^1.1.37",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "libphonenumber-metadata-generator": "^2.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/error-message": "2.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@mui/x-date-pickers": "^7.24.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@wcp/wario-shared": "workspace:*",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "es-toolkit": "^1.41.0",
    "geojson": "^0.5.0",
    "mailcheck": "^1.1.1",
    "motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-imask": "^7.6.1",
    "react-phone-number-input": "^3.4.13",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "reselect": "^5.1.1",
    "socket.io-client": "^4.8.1",
    "zod": "4.1.12"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}