{
  "name": "@wcp/wario-ux-shared",
  "version": "9.0.0",
  "description": "Shared UX elements across WARIO",
  "repository": {
    "type": "git",
    "url": "https://github.com/laviddichterman/wario-monorepo.git",
    "directory": "packages/wario-ux-shared"
  },
  "license": "GPL-3.0-or-later",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./common": {
      "types": "./dist/types/common/index.d.ts",
      "import": "./dist/common.js",
      "default": "./dist/common.js"
    },
    "./components": {
      "types": "./dist/types/components/index.d.ts",
      "import": "./dist/components.js",
      "default": "./dist/components.js"
    },
    "./containers": {
      "types": "./dist/types/containers/index.d.ts",
      "import": "./dist/containers.js",
      "default": "./dist/containers.js"
    },
    "./phone-metadata.custom.json": {
      "default": "./dist/phone-metadata.custom.json"
    },
    "./query": {
      "types": "./dist/types/query/index.d.ts",
      "import": "./dist/query.js",
      "default": "./dist/query.js"
    },
    "./redux": {
      "types": "./dist/types/redux/index.d.ts",
      "import": "./dist/redux.js",
      "default": "./dist/redux.js"
    },
    "./styled": {
      "types": "./dist/types/styled/index.d.ts",
      "import": "./dist/styled.js",
      "default": "./dist/styled.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "mkdir -p src/generated && pnpm run generate:metadata",
    "build": "vite build",
    "generate:metadata": "libphonenumber-metadata-generator src/generated/phone-metadata.custom.json --countries US,CA --extended --types mobile,fixed_line,voip",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/geojson": "^7946.0.16",
    "@types/mailcheck": "^1.1.37",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.2",
    "eslint-plugin-package-json": "^0.85.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.25",
    "globals": "^16.5.0",
    "libphonenumber-metadata-generator": "^2.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/error-message": "2.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@mui/x-date-pickers": "^8.17.0",
    "@mui/x-license": "^8.17.0",
    "@tanstack/react-query": "^5.90.11",
    "@wcp/wario-shared": "workspace:*",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "es-toolkit": "^1.41.0",
    "geojson": "^0.5.0",
    "mailcheck": "^1.1.1",
    "motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-imask": "^7.6.1",
    "react-phone-number-input": "^3.4.13",
    "socket.io-client": "^4.8.1",
    "zod": "4.1.12",
    "zustand": "^5.0.9"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
